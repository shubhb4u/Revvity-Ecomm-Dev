<?xml version="1.0" encoding="UTF-8"?>
<LightningComponentBundle xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>59.0</apiVersion>
    <isExposed>true</isExposed>
    <masterLabel>Self Service Portal</masterLabel>

    <targets>
        <target>lightningCommunity__Page</target>
        <target>lightningCommunity__Default</target>
    </targets>

    <targetConfigs>
        <targetConfig targets="lightningCommunity__Default">
            <property name="displayTitle" label="Title" type="String" default="Track your order" />
            <property name="tooltipText" label="Tooltip text" type="String" />
            <property name="tooltipImage"
                label="Tooltip image path (in Self-service image resource/from full path"
                type="String" />
            <property name="submitButtonText" label="Submit button text" type="String"
                default="Submit" />
        </targetConfig>
    </targetConfigs>
</LightningComponentBundle>